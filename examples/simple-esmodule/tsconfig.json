{
	"include": ["*.ts"],
	"compilerOptions": {
		"lib": ["ES5", "ES6", "ES7"],
		"target": "ES2020",
		"module": "ESNext",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"strict": true,
		"noEmit": true,
	}
}
